  <!-- Modules -->
  
  <import file="${basedir}/.idea/module_springbootdemo.xml"/>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.springbootdemo, clean.artifact.demo01:war_exploded, clean.artifact.springbootdemo:war_exploded" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.springbootdemo" description="build all modules"/>
  
  <target name="init.artifacts">
    <property name="artifacts.temp.dir" value="${basedir}/__artifacts_temp"/>
    <property name="artifact.output.demo01:war_exploded" value="${basedir}/out/artifacts/demo01_war_exploded"/>
    <property name="artifact.output.springbootdemo:war" value="${basedir}/target"/>
    <property name="artifact.output.springbootdemo:war_exploded" value="${basedir}/target/demo01-1.0-SNAPSHOT"/>
    <mkdir dir="${artifacts.temp.dir}"/>
    <property name="temp.jar.path.demo01-1.0-SNAPSHOT.war" value="${artifacts.temp.dir}/demo01-1.0-SNAPSHOT.war"/>
  </target>
  
  <target name="clean.artifact.demo01:war_exploded" description="clean demo01:war exploded artifact output">
    <delete dir="${artifact.output.demo01:war_exploded}"/>
  </target>
  
  <target name="clean.artifact.springbootdemo:war_exploded" description="clean springbootDemo:war exploded artifact output">
    <delete dir="${artifact.output.springbootdemo:war_exploded}"/>
  </target>
  
  <target name="artifact.demo01:war_exploded" depends="init.artifacts" description="Build &#39;demo01:war exploded&#39; artifact">
    <mkdir dir="${artifact.output.demo01:war_exploded}"/>
    <mkdir dir="${artifact.output.demo01:war_exploded}/WEB-INF"/>
    <mkdir dir="${artifact.output.demo01:war_exploded}/WEB-INF/classes"/>
    <copy todir="${artifact.output.demo01:war_exploded}/WEB-INF/classes">
      <fileset dir="${demo01.output.dir}"/>
    </copy>
    <mkdir dir="${artifact.output.demo01:war_exploded}/WEB-INF/lib"/>
